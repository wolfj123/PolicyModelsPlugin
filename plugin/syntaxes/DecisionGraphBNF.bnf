DECISION GRAPH BNF

<decision_graph> 				::= <top_level_node>*

<node> 							::= <top_level_node> | <sub_level_node>

#https://github.com/IQSS/DataTaggingLibrary/blob/develop/src/main/java/edu/harvard/iq/policymodels/parser/decisiongraph/DecisionGraphRuleParser.java#L205
<top_level_node>				::= "[" [<node_id>] 
									(<ask_node> 
									| <continue_node> 
									| <todo_node> 
									| <call_node> 
									| <reject_node> 
									| <set_node> 
									| <section_node>
									| <part_node>
									| <consider_node>
									| <when_node>
									| <import_node>) 
									"]"
									

#https://github.com/IQSS/DataTaggingLibrary/blob/develop/src/main/java/edu/harvard/iq/policymodels/parser/decisiongraph/DecisionGraphRuleParser.java#L44
<sub_level_node>				::= "{" 
									(<text_sub_node>
									| <info_sub_node>
									| <terms_sub_node>
									| <answers_sub_node>
									| <slot_sub_node>
									| <consider_answer_sub_node>
									| <consider_answers_sub_node>
									| <when_answer_sub_node>
									| <else_sub_node>)
									"}"


<text_sub_node>					::= "text" ":" <mulit_line_free_text>

<info_sub_node>					::= "title" ":" <mulit_line_free_text>

<terms_sub_node>				::= "term" ":" <term_sub_node>*

<term_sub_node>					::= <mulit_line_free_text> ":" <mulit_line_free_text>

<answers_sub_node>				::= "answers" ":" <answer_sub_node>+

<answer_sub_node>				::= <mulit_line_free_text> ":" <decision_graph>

<slot_sub_node>					::= "slot" ":" <slot>

<consider_answers_sub_node>		::=

<consider_answer_sub_node>		::= 

<when_answer_sub_node>			::=

<else_sub_node>					::=


