<mulit_line_free_text>			::= (<free_text> | <new_line>)*

<free_text>						::=  (<letter> | <digit> | ".,/~?!()@:#$%^&*_+-")+

<node_id> 						::= ">" <letter> (<letter> | <digit> | ".,/~?!()@:#$%^&*_+-" )*	"<"

<letter> 						::= "a" | "b" | ... | "z" | "A" | ... | "Z"

<digit> 						::= "0" | "1" | "2" | ... | "9"

<whitespaces>					::= <whitespace>*

<whitespace>					::= " "

#COMMENTS

<line_comment>					::= "<--" 

<multi_line_comment>			::= "<*" <free_text> (<newline> [<free_text>])* "*>"
