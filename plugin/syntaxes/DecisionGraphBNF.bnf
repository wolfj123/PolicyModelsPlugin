<slot> 							::= <slot_identifier> ("/"<slot_identifier>)*

<slot_identifier> 				::= <letter> (<letter> | <digit>)*  #should be the same one from policy space identifier

#https://datatagginglibrary.readthedocs.io/en/latest/decision-graphs/import-node.html
